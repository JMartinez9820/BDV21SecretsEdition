				MaxAirSpeed = clamp(MaxAirSpeed, MaxGroundSpeed, zm_maxhopspeed);
			}
		}
		else
		{
			//Friction											//In Dusk friction is applied regardless of wherever player is pressing
			if(Vel.XY.Length() < 1.f) { Vel.XY = (0, 0); }		//inputs,but I am implementing things slightly differently (and I allow
			Vel.XY *= 0.8f;										//max speed customization) so this feels better
		}
		
		//Acceleration
		Vel.XY += Acceleration;
		
		//Limiter
		Vel.XY = min(Vel.XY.Length(), MaxAirSpeed) * SafeUnit2(Vel.XY);
		
		//Sprite animation
		PlayIdle();
	}
	
