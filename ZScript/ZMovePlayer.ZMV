	Virtual void DuskFriction()
	{
		Let Player = Self.Player;
		UserCmd cmd = Player.cmd;
		
		//Going too slow, stop
		if(WaterLevel >= 2 || bNOGRAVITY)
		{
			if(Vel.Length() < 1.f && !cmd.sidemove && !cmd.forwardmove)
			{
				Vel.XY = (0, 0);
				return;
			}
		}
		else if(Vel.XY.Length() < 1.f && !cmd.sidemove && !cmd.forwardmove)
		{
			Vel.XY = (0, 0);
			return;
		}
		
		//User set friction value
		Float DFriction;
		if(WaterLevel >= 2)
			DFriction = 0.3f;
		else if(bNOGRAVITY)
			DFriction = 0.05f;
		else if(Player.OnGround)
			DFriction = zm_friction / 20.f;
		
		//Balance friction with inputs strength
		if(WaterLevel >= 2 || bNOGRAVITY)
		{
			Acceleration3 *= DFriction * (MaxGroundSpeed + 2.f);
			Vel *= 1 - DFriction;
		}
		else
		{
			Acceleration *= DFriction * (MaxGroundSpeed + 2.f);
			Vel.XY *= 1 - DFriction;
		}
	}
	
