	Override void HandleMovement()
	{
		let player = self.player;
		UserCmd cmd = Player.cmd;
		
		// [RH] Check for fast turn around
		if(player.cmd.buttons & BT_TURN180 && !(player.oldbuttons & BT_TURN180)) { player.turnticks = TURN180_TICKS; }

		// Handle movement
		if(reactiontime)
		{ // Player is frozen
			reactiontime--;
		}
		else
		{	
			ViewAngleDelta = cmd.Yaw * (360.0 / 65536.0); //needed for two other things
			
			if(Player.TurnTicks) //moved here to save many doubled lines
			{
				Player.TurnTicks--;
				A_SetAngle(Angle + (180.0 / TURN180_TICKS), SPF_INTERPOLATE);
			}
			else
			{
				A_SetAngle(Angle + ViewAngleDelta, SPF_INTERPOLATE);
				if(LedgeGrabbed) { Angle = clamp(Angle, LedgeAngle - 20, LedgeAngle + 20); }
			}
				
			/////////////////////////////////////////
			//Jumping
			
			if(zm_movetype == 1)
				BuildJump();
			else if(zm_movetype == 3)
				PainkillerJump();
			else if(zm_movetype == 5)
				UTJump();
			else
				CheckJump();
			
			/////////////////////////////////////////
			//Acrobatics triggers
			if(CVar.GetCVar("zm_doubletap", Player).GetBool()) { DoubleTapCheck(); }
			//Dashing parameters reset
			if(CoolerTrigger == 2 && (Player.OnGround || WaterLevel >= 2 || bNOGRAVITY))
			{
				if(DashNumber) { WJumpSpeed = DashNumber = 0; } //just in case
				CoolerTrigger = 1;
			}
			if(CoolerTrigger == 1)
			{
				DashCooler--;
				if(!DashCooler) { CoolerTrigger = 0; }
			}
			//Air Dash
			if(zm_airdash)
			{
				if(ADashCooler) { ADashCooler--; }
				if(ADashTargetSpeed) { AirDashFriction(); }
			}
			//Wall Jumping
			if(CheckForWJump) { DashInitiator(); }
			//Wall Slide
			if(CheckForWSlide) { WallSlideInitiator(); }
			//Crouchsliding
			if(zm_crouchslide) { CSlideInitiator(); }
			
			/////////////////////////////////////////
			//Actual Movement selection
			
			if(GrappleVel.Length())
			{
				GrapplingMove();
			}
			else if(CanWSlide)
			{
				WallSlideMove();
			}
			else if(LedgeGrabbed)
			{
				LedgeGrab();
			}
			else if(WaterLevel >= 2)
			{
				if(!zm_movetype)
					DoomWaterMove();
				else if(zm_movetype == 1)
					BuildWaterMove();
				else if(zm_movetype == 2)
					DuskWaterMove();
				else if(zm_movetype == 3)
					PainkillerWaterMove();
				else if(zm_movetype == 4)
					QuakeWaterMove();
				else if(zm_movetype == 5)
					UTWaterMove();
				
				if(zm_ledgegrab) { LedgeGrabInitiator(); }
			}
			else if(bNOGRAVITY)
			{
				if(!zm_movetype)
					DoomFlyMove();
				else if(zm_movetype == 1)
					BuildFlyMove();
				else if(zm_movetype == 2)
					DuskFlyMove();
				else if(zm_movetype == 3)
					PainkillerFlyMove();
				else if(zm_movetype == 4)
					QuakeFlyMove();
				else if(zm_movetype == 5)
					UTFlyMove();
			}
			else if(CanCSlide)
			{
				if(zm_cslidetype)
					QSlideMove();
				else
					CSlideMove();
				
				if(zm_movetype == 1) { LandingVelZ = 0.f; }
			}
			else
			{
				if(!zm_movetype)
					DoomHandleMove();
				else if(zm_movetype == 1)
					BuildHandleMove();
				else if(zm_movetype == 2)
					DuskHandleMove();
				else if(zm_movetype == 3)
					PainkillerHandleMove();
				else if(zm_movetype == 4)
					QuakeHandleMove();
				else
					UTHandleMove();
			}
			
			if(Player.Cheats & CF_REVERTPLEASE != 0)
			{
				Player.Cheats &= ~CF_REVERTPLEASE;
				Player.Camera = Player.Mo;
			}
				
			CheckMoveUpDown();
		}
	}
	
